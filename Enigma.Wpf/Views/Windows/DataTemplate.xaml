<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:FormViewModels="clr-namespace:Enigma.Wpf.ViewModels.Forms"
    xmlns:Forms="clr-namespace:Enigma.Wpf.Views.Forms"
    xmlns:ViewModels="clr-namespace:Enigma.Wpf.ViewModels"
    xmlns:Views="clr-namespace:Enigma.Wpf.Views"
    xmlns:WindowViewModels="clr-namespace:Enigma.Wpf.ViewModels.Windows"
    xmlns:WindowViews="clr-namespace:Enigma.Wpf.Views.Windows">
    <BooleanToVisibilityConverter x:Key="BoolToVisibilityConverter" />

    <DataTemplate DataType="{x:Type WindowViewModels:MainWindowViewModel}">
        <WindowViews:MainWindow />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:InitialFormViewModel}">
        <Views:InitialForm />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:MainAppViewModel}">
        <Views:MainAppView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type FormViewModels:ImportFormViewModel}">
        <Forms:ImportFormView />
    </DataTemplate>
</ResourceDictionary>
