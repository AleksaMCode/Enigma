<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ViewModels="clr-namespace:Enigma.EnigmaWpf.ViewModels"
    xmlns:Views="clr-namespace:Enigma.EnigmaWpf.Views">
    <DataTemplate DataType="{x:Type ViewModels:MainWindowViewModel}">
        <Views:MainWindow />
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:InitialFormViewModel}">
        <Views:InitialForm />
    </DataTemplate>
</ResourceDictionary>
